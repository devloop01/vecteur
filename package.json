{
	"name": "vecteur",
	"version": "0.2.0",
	"author": "Sikriti Dakua",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/devloop01/vecteur"
	},
	"keywords": [
		"math",
		"vector",
		"typescript",
		"javascript"
	],
	"type": "module",
	"main": "./dist/cjs/index.cjs",
	"module": "./dist/esm/index.js",
	"types": "./dist/types/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/types/index.d.ts",
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.cjs"
		},
		"./2d": {
			"types": "./dist/types/2d/index.d.ts",
			"import": "./dist/esm/2d/index.js",
			"require": "./dist/cjs/2d/index.cjs"
		},
		"./3d": {
			"types": "./dist/types/3d/index.d.ts",
			"import": "./dist/esm/3d/index.js",
			"require": "./dist/cjs/3d/index.cjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"clean": "rm -rf dist/*",
		"build": "pnpm clean && rollup -c",
		"prepublishOnly": "pnpm build",
		"changeset": "changeset",
		"release": "changeset publish",
		"test": "vitest"
	},
	"devDependencies": {
		"@changesets/changelog-github": "^0.5.0",
		"@changesets/cli": "^2.27.9",
		"@types/node": "^22.8.4",
		"@typescript-eslint/eslint-plugin": "^8.12.2",
		"@typescript-eslint/parser": "^8.12.2",
		"esbuild": "^0.24.0",
		"eslint": "^9.13.0",
		"prettier": "^3.3.3",
		"rollup": "^4.24.3",
		"rollup-plugin-dts": "^6.1.1",
		"rollup-plugin-esbuild": "^6.1.1",
		"typescript": "^5.6.3",
		"vitest": "^2.1.4"
	},
	"packageManager": "pnpm@9.9.0"
}
